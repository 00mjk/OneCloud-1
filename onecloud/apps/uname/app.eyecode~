function uname_run($params=null) {
	if(count($params) == 0) {
		eyeX('print',array('oneye'));
	} else {
		if($params[0] == '-a') {
			eyeX('print',array('oneye ' . ONEYE_VERSION . ' (' . EYEOS_VERSION . ') ' . EYEOS_HOSTNAME));
		} elseif($params[0] == '-s') {
			eyeX('print',array('oneye'));
		} elseif($params[0] == '-v') {
			eyeX('print',array(EYEOS_VERSION));
		} elseif($params[0] == '-n') {
			eyeX('print',array(EYEOS_HOSTNAME));
		}
	}
	proc('end');
}
?>